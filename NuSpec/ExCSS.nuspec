    <owners>SR2, Sergio Reider Benabib</owners>
    <projectUrl>https://github.com/SR2IT/ExCSS</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/SR2IT/ExCSS/master/ExCSS.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/SR2IT/ExCSS/master/license.txt</licenseUrl>
    <copyright>Copyright © 2016, Sergio Reider Benabib. Copyright © 2012-2016, Tyler Brinks.</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <language>en-US</language>
    <tags>ExCSS, CSS, Style, Stylesheet, Parse, Lexer, Scanner, Linq</tags>
    <references>
      <reference file="ExCSS.dll" />
    </references>
    <releaseNotes>
v2.0.7 - Retargeted to be compatible with most portable targets.
v1.0.1 - v2.0.6 - Initial release
    </releaseNotes>
  </metadata>
  <files>
      <file src="..\ExCSS\bin\Release\ExCSS.dll" target="lib\portable-net45+sl50+netcore45+wpa81+wp8" />
